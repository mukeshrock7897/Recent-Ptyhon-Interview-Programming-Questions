def count(listOfTuple):
    import pdb;pdb.set_trace()
    count_map = {}
    for i in listOfTuple:
        count_map[i] = count_map.get(i, 0) +1
        print("i Value",i ,"Count_Map Value",count_map  ,"Count_Map i Value",count_map[i] , "Count_Map Get Value",count_map.get(i,0)+1 , end="\n")
    print(count_map) 


listOfTuple = [('a', 'e'), ('b', 'x'),('a', 'e'), ('b', 'x')] 

count(listOfTuple) 


Output-

{('a', 'e'): 2, ('b', 'x'): 2}
